&lt;html&gt;&lt;head&gt;&lt;meta name=&quot;qrichtext&quot; content=&quot;1&quot; /&gt;&lt;style type=&quot;text/css&quot;&gt;
p, li { white-space: pre-wrap; }
&lt;/style&gt;&lt;/head&gt;&lt;body style=&quot; font-family:'Ubuntu'; font-size:11pt; font-weight:400; font-style:normal;&quot;&gt;
&lt;p align=&quot;justify&quot; style=&quot; margin-top:0px; margin-bottom:10px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-size:12pt; font-weight:600; color:#f9711d;&quot;&gt;Meshes&lt;/span&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt; folder contains two child directories: &lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600; color:#f9711d;&quot;&gt;visual &lt;/span&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt;(uesd for visualization) and &lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600; color:#f9711d;&quot;&gt;collision &lt;/span&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt;(used for collision checking). &lt;/span&gt;&lt;/p&gt;
&lt;p align=&quot;justify&quot; style=&quot; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-size:12pt; font-weight:600;&quot;&gt;Steps to create &lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600; color:#f9711d;&quot;&gt;meshes&lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600;&quot;&gt; folder:&lt;/span&gt;&lt;/p&gt;
&lt;p align=&quot;justify&quot; style=&quot;-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-size:12pt; font-weight:600;&quot;&gt;&lt;br /&gt;&lt;/p&gt;
&lt;p align=&quot;justify&quot; style=&quot; margin-top:0px; margin-bottom:10px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:20px;&quot;&gt;&lt;span style=&quot; font-size:14pt; font-weight:600;&quot;&gt;Step 1.&lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600;&quot;&gt;  &lt;/span&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt;Rename all STL files in ROS style as follows:&lt;/span&gt;&lt;/p&gt;
&lt;p align=&quot;justify&quot; style=&quot; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:50px;&quot;&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt;Base.stl  &lt;/span&gt;&lt;span style=&quot; font-size:12pt; font-weight:600;&quot;&gt;→&lt;/span&gt;&lt;span style=&quot; font-size:12pt;&quot;&gt; base_link.stl&lt;/span&gt;&lt;/p&gt;
